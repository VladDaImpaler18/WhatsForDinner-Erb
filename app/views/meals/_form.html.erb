<%= form_for meal do |f| %>
<%# title:sting, category:sting, area:sting, instructions:json image:binary, tags:json, ingredients:json, source:string%>
    <%= f.label :title %>
    <%= f.text_field :title %>
    
    <%= f.label :category %>
    <%= f.text_field :category %>

    <%= f.text_area :instructions %>

    <%= f.label :tags %>

    <%# Incredients section is broken into two parts%>
    <%= f.label :ingredients %>
    <%= f.text_field :ingredients %>
    <%# Text field with amount | Text field for ingredient %>

    <%= f.label :source %>
    <%= f.text_field :source %>

    <%= f.submit %>

<% end %>